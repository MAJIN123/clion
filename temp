#include<stdio.h>
#include<iostream>
using namespace std;
#include<queue>
#include<fstream>
#include<string>
#include<sstream>
#include<string.h>
#include<vector>
const int Time=1;		//定义轮转的每个进程的执行时间
struct Mat{
	int No;//进程
	int arr_time;//到来时间
	int exu_time;//运行时间
	int priority;//优先级
};
int main(){
	fstream in("hh.txt");
	if(!in){
		cout<<"Can not open this file!"<<endl;
	}
	queue<Mat > Bc;						//存放几条进程的所有数据
	string s;getline(in,s);
	for(s;getline(in,s);){
		istringstream sin(s);
		int count=0;int a[4];
		Mat temp;
		for(int ia;sin>>ia;)
			a[count++]=ia;
		temp.No=a[0];
		temp.arr_time=a[1];
		temp.exu_time=a[2];
		temp.priority=a[3];
		Bc.push(temp);					//读进队列
	}
	while(Bc.size()!=NULL){
	Mat temp;
	temp=Bc.front();
	temp.exu_time-=Time;
	cout<<"p"<<temp.No<<" ";
	Bc.pop();
	if(temp.exu_time!=0)
		Bc.push(temp);
	}
}
